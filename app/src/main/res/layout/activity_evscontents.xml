<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/evscontents_bg"
    android:orientation="vertical"
    tools:context=".evscontents">

    <TextView
        android:id="@+id/textView17"
        android:layout_width="101dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:text="@string/evs_heading"
        android:textColor="#5803AC"
        android:textSize="54sp"
        android:textStyle="bold" />

    <ImageButton
        android:id="@+id/evstosub"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/ic_baseline_arrow_back_ios_24"
        android:backgroundTint="#000000"
        android:textSize="40sp"
        android:translationX="10dp"
        android:translationY="-60dp"
        tools:ignore="TextContrastCheck,SpeakableTextPresentCheck,ContentDescription" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="90dp"
        android:layout_height="85dp"
        android:translationX="330dp"
        android:translationY="-140dp"
        app:srcCompat="@drawable/logo"
        tools:ignore="SpeakableTextPresentCheck, ContentDescription,ImageContrastCheck" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="198dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/lily_script_one"
        android:text="@string/animals"

        android:textColor="#FB0008"
        android:textSize="50sp"
        android:translationX="30dp"
        android:translationY="-25dp" />

    <ImageButton
        android:id="@+id/evstoani"
        android:layout_width="94dp"
        android:layout_height="89dp"
        android:background="@drawable/evs_proceedbutton"
        android:translationX="280dp"
        android:translationY="-100dp"
        tools:ignore="TextContrastCheck,SpeakableTextPresentCheck,ContentDescription" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="198dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/lily_script_one"
        android:text="@string/evscontents_festivals"
        android:textColor="#FB0008"
        android:textSize="50sp"
        android:translationX="30dp"
        android:translationY="-75dp" />

    <ImageButton
        android:id="@+id/evstofes"
        android:layout_width="94dp"
        android:layout_height="89dp"
        android:background="@drawable/evs_proceedbutton"
        android:translationX="280dp"
        android:translationY="-140dp"
        tools:ignore="TextContrastCheck,SpeakableTextPresentCheck,ContentDescription" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="198dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/lily_script_one"
        android:text="@string/evs_human_body"
        android:textColor="#FB0008"
        android:textSize="50sp"
        android:translationX="30dp"
        android:translationY="-135dp" />

    <ImageButton
        android:id="@+id/evstohum"
        android:layout_width="94dp"
        android:layout_height="89dp"
        android:background="@drawable/evs_proceedbutton"
        android:translationX="280dp"
        android:translationY="-245dp"
        tools:ignore="TextContrastCheck,SpeakableTextPresentCheck,ContentDescription" />




</LinearLayout>